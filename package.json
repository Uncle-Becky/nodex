{
  "name": "nodal-agent-system",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host --open",
    "dev:debug": "vite --host --open --debug",
    "dev:agents": "npm run dev -- --mode agents",
    "build": "npm run typecheck && vite build",
    "build:analyze": "npm run build -- --mode analyze",
    "build:production": "npm run lint:fix && npm run typecheck && vite build --mode production",
    "preview": "vite preview --host",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --max-warnings 0",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "lint:watch": "nodemon --watch src --ext ts,tsx --exec \"npm run lint\"",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "clean": "rimraf dist node_modules/.vite",
    "clean:full": "rimraf dist node_modules package-lock.json && npm install",
    "agents:demo": "node scripts/demo-runner.js",
    "agents:test": "node scripts/agent-tester.js",
    "agents:benchmark": "node scripts/benchmark-agents.js",
    "agents:analyze": "node scripts/analyze-system.js",
    "workers:test": "node scripts/test-workers.js",
    "system:health": "node scripts/health-check.js",
    "system:monitor": "node scripts/monitor-system.js",
    "docs:generate": "node scripts/generate-docs.js",
    "setup:dev": "node scripts/setup-development.js",
    "deploy:prepare": "npm run build:production && node scripts/prepare-deployment.js",
    "postinstall": "node scripts/post-install.js",
    "auto:define": "node scripts/auto-define.js",
    "auto:fix": "npm run auto:define && npm run lint:fix && npm run typecheck",
    "type:analyze": "node scripts/type-analyzer.js",
    "type:fix": "npm run type:analyze && npm run auto:define && npm run typecheck",
    "smart:fix": "node scripts/smart-type-fixer.js"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.8",
    "@tailwindcss/vite": "^4.1.8",
    "@xyflow/react": "^12.6.4",
    "file-saver": "^2.0.5",
    "react": "^19.0.1",
    "react-dom": "^19.0.1",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/file-saver": "^2.0.7",
    "@types/node": "^22.15.30",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "@vitejs/plugin-react": "^4.5.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "globals": "^14.0.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.4",
    "postcss-import": "^16.1.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4.1.8",
    "typescript": "^5.8.3",
    "vite": "^6.3.6"
  },
  "browserslist": [
    "defaults",
    "maintained node versions",
    "not dead"
  ]
}
